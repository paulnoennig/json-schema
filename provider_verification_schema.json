{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "provider_found",
    "office_found",
    "provider_is_active",
    "corrected_provider",
    "corrected_office",
    "notes"
  ],
  "properties": {
    "provider_found": { "type": "boolean" },
    "office_found": { "type": "boolean" },
    "provider_is_active": { "type": "boolean" },
    "corrected_provider": {
      "type": "object",
      "required": ["first", "last", "prefix", "suffix", "type"],
      "properties": {
        "first": { "type": "string" },
        "last": { "type": "string" },
        "prefix": { "type": "string" },
        "suffix": { "type": "string" },
        "type": {
          "type": "string",
          "enum": [
            "Dentist",
            "Chiropractor",
            "MD",
            "ENT",
            "Speech Pathologist",
            "Physical Therapist",
            "Other"
          ]
        }
      },
      "additionalProperties": false
    },
    "corrected_office": {
      "type": "object",
      "required": [
        "name",
        "address_line_1",
        "address_line_2",
        "city",
        "state",
        "zip",
        "phone",
        "mobile",
        "fax",
        "email",
        "website"
      ],
      "properties": {
        "name": { "type": "string" },
        "address_line_1": { "type": "string" },
        "address_line_2": { "type": "string" },
        "city": { "type": "string" },
        "state": { "type": "string" },
        "zip": { "type": "string" },
        "phone": { "type": "string" },
        "mobile": { "type": "string" },
        "fax": { "type": "string" },
        "email": { "type": "string" },
        "website": { "type": "string" }
      },
      "additionalProperties": false
    },
    "notes": { "type": "string" }
  },
  "additionalProperties": false
}
